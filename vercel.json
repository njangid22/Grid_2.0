{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Flipkart_Grid_6_0.settings",
    "DATABASE_URL": "your_database_url",
    "DJANGO_DEBUG": "False"
  }
}
